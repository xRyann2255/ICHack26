FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceFeatureExtractDict;
}

southken.stl
{
    extractionMethod    extractFromSurface;
    
    extractFromSurfaceCoeffs
    {
        includedAngle   150;  // edges sharper than this get extracted
    }
    
    writeObj    yes;
}
